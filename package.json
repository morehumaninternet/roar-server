{
  "scripts": {
    "build": "npm run clear && node build.js",
    "build:ensure": "{ [ -d compiled ] && [ -d css ] && [ -d html ] ; } || npm run build",
    "clear": "concurrently 'rimraf compiled' 'rimraf css' 'rimraf html'",
    "dev": "NODE_ENV=dev npm run build -- --watch",
    "lint": "{ find src -name '*.ts' | xargs tslint --project tsconfig.json --config tslint.json ; }",
    "knex": "knex -- \"$@\"",
    "postinstall": "npm run build",
    "start": "node -r source-map-support/register compiled/entry.js",
    "test": "npm run build:ensure && nyc $NYC_OPTS --exclude src/types.ts --exclude src/__test__ mocha $MOCHA_OPTS --require test_setup.js 'compiled/**/*.test.js'",
    "test:coverage": "NYC_OPTS='--reporter=lcov' npm test",
    "views": "node views.js"
  },
  "dependencies": {
    "@mailchimp/mailchimp_marketing": "3.0.27",
    "@sentry/node": "5.27.3",
    "@sentry/tracing": "5.27.3",
    "axios": "^0.21.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-register": "6.26.0",
    "cheerio": "1.0.0-rc.3",
    "dotenv": "8.2.0",
    "knex": "0.21.7",
    "knex-upsert": "0.0.4",
    "koa": "2.13.0",
    "koa-body": "4.0.7",
    "koa-cookie": "1.0.0",
    "koa-helmet": "4.0.0",
    "koa-passport": "4.1.3",
    "koa-redis": "4.0.1",
    "koa-router": "7.2.0",
    "koa-send": "5.0.0",
    "koa-session": "6.0.0",
    "lodash": "4.17.20",
    "mocha.parallel": "0.15.3",
    "node-uuid": "1.4.8",
    "parse-domain": "3.0.3",
    "passport-twitter": "1.0.4",
    "pg": "8.2.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "twit": "2.2.11"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "3.4.34",
    "@types/chai-http": "0.0.30",
    "@types/cheerio": "0.22.22",
    "@types/dotenv": "8.2.0",
    "@types/jsdom": "16.2.5",
    "@types/knex": "0.16.1",
    "@types/knex-cleaner": "1.3.0",
    "@types/koa": "2.0.39",
    "@types/koa-bodyparser": "4.2.2",
    "@types/koa-cookie": "1.0.0",
    "@types/koa-passport": "4.0.2",
    "@types/koa-redis": "4.0.0",
    "@types/koa-router": "7.0.22",
    "@types/koa-send": "4.1.2",
    "@types/koa-session": "5.10.2",
    "@types/lodash": "4.14.62",
    "@types/mocha": "2.2.35",
    "@types/node": "10.12.2",
    "@types/node-uuid": "0.0.28",
    "@types/passport-twitter": "1.0.36",
    "@types/pg": "7.14.3",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/sinon": "9.0.8",
    "@types/sinon-chai": "2.7.27",
    "@types/supertest": "2.0.9",
    "@types/twit": "2.2.28",
    "chai": "3.4.1",
    "chai-http": "4.3.0",
    "chalk": "4.1.0",
    "chokidar": "3.4.3",
    "clear-module": "4.1.1",
    "concurrently": "5.3.0",
    "husky": "4.3.0",
    "jsdom": "16.4.0",
    "knex-cleaner": "1.3.1",
    "mkdirp": "1.0.4",
    "mocha": "7.1.2",
    "nyc": "15.1.0",
    "prettier": "2.1.2",
    "pretty-quick": "3.1.0",
    "rimraf": "3.0.2",
    "sass": "1.27.1",
    "sinon": "9.2.0",
    "sinon-chai": "2.8.0",
    "source-map-support": "0.5.19",
    "strip-ansi": "6.0.0",
    "supertest": "4.0.2",
    "tee": "0.2.0",
    "tslint": "5.8.0",
    "tslint-immutable": "4.4.0",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": "14.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && pretty-quick --staged --pattern \"src/**/*.*(ts|tsx)\""
    }
  }
}
